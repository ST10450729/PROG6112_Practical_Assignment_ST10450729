package student;

/**
 *
 * @author rakwa
 */
public class Student {
    private String id;
    private String name;
    private int age;
    private String course;
    
    public Student(String id, String name, int age, String course){
        this.id = id;
        this.name = name;
        this.age = age;
        this.course = course;
        
    }
    
    public String getId() {
        return id;
    }
    
    public void setId(String id) {
        this.id = id;
    }
    
    public String getName() {
        return name;
    }
    
    public void setName(String name) {
        this.name = name;
    }
    
    public int getAge() {
        return age;
    }
    
    public void setAge(int age) {
        this.age = age;
    }
    
    public String getCourse() {
        return course;
    }
    
    public void setCourse() {
        this.course = course;
    }  
    
}
